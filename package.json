    "@testing-library/user-event": "^14.5.2",
    "@types/node": "^20.17.7",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@vitejs/plugin-react": "^4.3.4",
    "jsdom": "^25.0.1",
